{
  "name": "Legal Compliance Frontend Dev",
  "image": "mcr.microsoft.com/devcontainers/javascript-node:1-20-bookworm",
  "customizations": {
    "codespaces": {
      "openFiles": [
        "README.md"
      ]
    },
    "vscode": {
      "settings": {},
      "extensions": [
        "dbaeumer.vscode-eslint"
      ]
    }
  },
  "updateContentCommand": "cd frontend-react && npm install",
  "postAttachCommand": {
    "frontend": "cd frontend-react && npm run dev"
  },
  "portsAttributes": {
    "5173": {
      "label": "Frontend",
      "onAutoForward": "openPreview"
    }
  },
  "forwardPorts": [
    5173
  ]
}
